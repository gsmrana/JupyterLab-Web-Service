[Unit]
Description=JupyterLab Web Service
After=network.target

[Service]
ExecStart=/srv/JupyterLab-Web-Service/.venv/bin/jupyter-lab --ip=0.0.0.0 --no-browser --allow-root
WorkingDirectory=/srv/JupyterLab-Web-Service/Notebooks
Restart=always
RestartSec=3
User=root
Group=root

#StandardOutput=null
#StandardError=journal
#Environment=PYTHONUNBUFFERED=1

[Install]
WantedBy=multi-user.target
